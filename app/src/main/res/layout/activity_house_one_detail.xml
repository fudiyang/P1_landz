<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context="landz.fdy.com.p1_landz.landz.UI.welcome.onlinehouse.HouseOneDetailActivity">

    <landz.fdy.com.p1_landz.landz.UI.welcome.View.HouseDetailTitleBar
        android:id="@+id/titleBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/titleBar">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <fragment
                android:id="@+id/detail_fragment2"
                android:name="landz.fdy.com.p1_landz.landz.UI.welcome.fragment.DetailFragment_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <fragment
                android:id="@+id/detail_fragment3"
                android:name="landz.fdy.com.p1_landz.landz.UI.welcome.fragment.DetailFragment_3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <fragment
                android:id="@+id/detail_fragment4"
                android:name="landz.fdy.com.p1_landz.landz.UI.welcome.fragment.DetailFragment_4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>

            <fragment
                android:id="@+id/detail_fragment1"
                android:name="landz.fdy.com.p1_landz.landz.UI.welcome.fragment.DetailFragment_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <fragment
                android:id="@+id/detail_fragment7"
                android:name="landz.fdy.com.p1_landz.landz.UI.welcome.fragment.DetailFragment_7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <fragment
                android:id="@+id/detail_fragment5"
                android:name="landz.fdy.com.p1_landz.landz.UI.welcome.fragment.DetailFragment_5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>

            <fragment
                android:id="@+id/detail_fragment6"
                android:name="landz.fdy.com.p1_landz.landz.UI.welcome.fragment.DetailFragment_6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="#7F000000"/>

        </LinearLayout>
    </ScrollView>
      <landz.fdy.com.p1_landz.landz.UI.welcome.View.HouseDetailBottomView
          android:id="@+id/bottomView"
          android:layout_width="match_parent"
          android:layout_height="55dp"
          android:layout_alignParentBottom="true"/>

</RelativeLayout>
