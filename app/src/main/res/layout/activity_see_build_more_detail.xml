                android:layout_height="wrap_content"/>
            <fragment
                android:id="@+id/seebuild_detail_fragment_6"
                android:name="landz.fdy.com.p1_landz.landz.UI.welcome.seebuild_fragment.SeeBuild_Fragment_6"
