                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:background="#7F000000"/>

        </LinearLayout>
    </ScrollView>m
    <landz.fdy.com.p1_landz.landz.UI.welcome.seebuild.SeeBuildBottomView
